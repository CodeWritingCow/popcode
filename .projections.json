{
  "src/components/*.jsx": {
    "type": "component",
    "template": "import React from 'react';\n\nclass {camelcase|capitalize} extends React.Component {open}\n  render() {open}\n  {close}\n{close};\n\nexport default {camelcase|capitalize};"
  },
  "src/store.js": {
    "type": "store"
  },
  "src/actions/*.js": {
    "type": "action"
  },
  "src/reducers/*.js": {
    "type": "reducer"
  },
  "src/sagas/*.js": {
    "type": "saga"
  },
  "src/clients/*.js": {
    "type": "client"
  },
  "test/unit/*.js": {
    "type": "test",
    "template": "import test from 'tape';\n\ntest('{}', (assert) => {open}\n{close});"
  },
  "src/validations/*.js": {"type": "validation"},
  "src/static/*": {"type": "static"},
  "src/css/application.css": {"type": "stylesheet"},
  "locales/*/translation.json": {"type": "locale"}
}
